{
  "builds": [
    {
      "src": "django2/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.9"}
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "django2/wsgi.py"
    }
  ],
  "functions": {
    "api/**/*.py": {
      "excludeFiles": "django2/static/**"
    }
  }
}
